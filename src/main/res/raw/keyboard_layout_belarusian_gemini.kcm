#
# Belarusian keyboard layout.
# This is a typical Belarusian PC keyboard layout.
# As an added convenience, English characters are accessible using alt: (Alt Gr).
#

type OVERLAY

### ROW 1

key ESCAPE {
    base:                               fallback BACK
    alt, meta:                          fallback HOME
    ctrl:                               fallback MENU
}

key 1 {
    label:                              '1'
    base:                               '1'
    shift:                              '!'
    alt:                                '!'
    fn:                                 '@'
}

key 2 {
    label:                              '2'
    base:                               '2'
    shift:                              '"'
    alt:                                '@'
    fn:                                 '#'
}

key 3 {
    label:                              '3'
    base:                               '3'
    alt:                                '\u2116'
    shift:                              '\u00a3'
    fn:                                 '\\'
}

key 4 {
    label:                              '4'
    base:                               '4'
    shift:                              '$'
    alt:                                ';'
    fn:                                 '\u20ac'
}

key 5 {
    label:                              '5'
    base:                               '5'
    shift:                              '%'
    alt:                                '%'
    fn:                                 '<'
}

key 6 {
    label:                              '6'
    base:                               '6'
    alt:                                ':'
    shift:                              '^'
    fn:                                 '>'
}

key 7 {
    label:                              '7'
    base:                               '7'
    alt:                                '?'
    shift:                              '&'
    fn:                                 '['
}

key 8 {
    label:                              '8'
    base:                               '8'
    shift:                              '*'
    alt:                                '*'
    fn:                                 ']'
}

key 9 {
    label:                              '9'
    base:                               '9'
    shift:                              '('
    alt:                                '('
    fn:                                 '{'
}

key 0 {
    label:                              '0'
    base:                               '0'
    shift:                              ')'
    alt:                                ')'
    fn:                                 '}'
}

key DEL {
    base:			                	fallback DEL
    shift:        			            fallback FORWARD_DEL
}

### ROW 2

key Q {
    label:                              '\u0419'
    base:                               '\u0439'
    shift, capslock:                    '\u0419'
    alt:                                'q'
    alt+shift, alt+capslock:            'Q'
}

key W {
    label:                              '\u0426'
    base:                               '\u0446'
    shift, capslock:                    '\u0426'
    alt:                                'w'
    alt+shift, alt+capslock:          'W'
}

key E {
    label:                              '\u0423'
    base:                               '\u0443'
    shift, capslock:                    '\u0423'
    alt:                                'e'
    alt+shift, alt+capslock:          'E'
}

key R {
    label:                              '\u041a'
    base:                               '\u043a'
    shift, capslock:                    '\u041a'
    alt:                                'r'
    alt+shift, alt+capslock:          'R'
    fn:                                 '_'
}

key T {
    label:                              '\u0415'
    base:                               '\u0435'
    shift, capslock:                    '\u0415'
    alt:                                't'
    alt+shift, alt+capslock:          'T'
}

key Y {
    label:                              '\u041d'
    base:                               '\u043d'
    shift, capslock:                    '\u041d'
    alt:                                'y'
    alt+shift, alt+capslock:          'Y'
    fn:                                 '\u0445'
    fn+shift:                           '\u0425'
}

key U {
    label:                              '\u0413'
    base:                               '\u0433'
    shift, capslock:                    '\u0413'
    alt:                                'u'
    alt+shift, alt+capslock:          'U'
    fn:                                 '\u0027'
    fn+shift:                           '\u0027'
}

key I {
    label:                              '\u0428'
    base:                               '\u0448'
    shift, capslock:                    '\u0428'
    alt:                                'i'
    alt+shift, alt+capslock:          'I'
    fn:                                 '+'
}

key O {
    label:                              '\u040E'
    base:                               '\u045E'
    shift, capslock:                    '\u040E'
    alt:                                'o'
    alt+shift, alt+capslock:          'O'
    fn:                                 '-'
}

key P {
    label:                              '\u0417'
    base:                               '\u0437'
    shift, capslock:                    '\u0417'
    alt:                                'p'
    alt+shift, alt+capslock:          'P'
    fn:                                 '='
}

key ENTER {
    label:                              '\n'
    base:                               '\n'
}

