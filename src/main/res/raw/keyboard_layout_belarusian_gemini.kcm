    base:                               '\u0440'
    shift, capslock:                    '\u0420'
    alt:                                'h'
    alt+shift, alt+capslock:          'H'
    fn:                                 '\u044d'
    fn+shift:                           '\u042d'
}

key J {
    label:                              '\u041e'
    base:                               '\u043e'
    shift, capslock:                    '\u041e'
    alt:                                'j'
    alt+shift, alt+capslock:          'J'
    fn:                                 '\u044e'
    fn+shift:                           '\u042e'
